                rdfs:domain <http://erlangen-crm.org/140617/E5_Event> ;
                rdfs:range <http://erlangen-crm.org/140617/E5_Event> ;
                rdfs:label "Preceding Event"@en .


###  http://purl.org/ontology/scc#precedingUnit
:precedingUnit rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :predecessor ;
               owl:inverseOf :succeedingUnit ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :AdministrativeArea
                                           :administrative_unit
                                         )
                           ] ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( :AdministrativeArea
                                          :administrative_unit
                                        )
                          ] .


###  http://purl.org/ontology/scc#predecessor
:predecessor rdf:type owl:ObjectProperty ;
             rdfs:label "Predecessor"@en .


###  http://purl.org/ontology/scc#province_is_composed_of
:province_is_composed_of rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf :is_composed_of ;
                         rdfs:domain :AdministrativeArea ;
                         rdfs:range :administrative_unit ;
                         rdfs:label "province is composed of"@en .


###  http://purl.org/ontology/scc#renomination_name
:renomination_name rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :resulting_to ;
                   rdfs:domain :renomination ;
                   rdfs:range [ rdf:type owl:Restriction ;
                                owl:onProperty :changed_name ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:string
                              ] ;
                   rdfs:label "renomination name"@en .


###  http://purl.org/ontology/scc#resulting_to
:resulting_to rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://erlangen-crm.org/120111/P92_brought_into_existence> ;
              rdfs:domain :Change ;
              rdfs:range :religious_administrative_unit ;
              rdfs:label "resulting to"@en .


###  http://purl.org/ontology/scc#succeedingUnit
:succeedingUnit rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :successor ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :AdministrativeArea
                                            :administrative_unit
                                          )
                            ] ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( :AdministrativeArea
                                           :administrative_unit
                                         )
                           ] .


###  http://purl.org/ontology/scc#successor
:successor rdf:type owl:ObjectProperty ;
           rdfs:label "Successor"@en .


###  http://purl.org/ontology/scc#transfer_seat
:transfer_seat rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :resulting_to ;
               rdfs:domain :transfer ;
               rdfs:range :city ;
               rdfs:label "transfer seat"@en .


###  http://purl.org/ontology/scc#unit_level_change
:unit_level_change rdf:type owl:ObjectProperty ;
                   rdfs:domain :religious_administrative_unit ;
                   rdfs:range :religious_administrative_unit ;
                   rdfs:label "unit level change"@en .


